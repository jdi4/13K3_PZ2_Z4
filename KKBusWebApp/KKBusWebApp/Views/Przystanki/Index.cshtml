@model IEnumerable<KKBusWebApp.Models.PRZYSTANKI>

@{
    ViewBag.Title = "Edycja przystanków";
}

<h2>Edycja przystanków</h2>

<p>
    @Html.ActionLink("Dodaj nowy", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PRZY_NAZWA)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PRZY_GPS_LONGITUDE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PRZY_GPS_LATITUDE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PRZY_ZADANIE)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PRZY_NAZWA)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PRZY_GPS_LONGITUDE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PRZY_GPS_LATITUDE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PRZY_ZADANIE)
        </td>
        <td>
            @Html.ActionLink("Edytuj", "Edit", new { id=item.PRZY_ID }) |
            @*@Html.ActionLink("Details", "Details", new { id=item.PRZY_ID }) |*@
            @Html.ActionLink("Usuń", "Delete", new { id=item.PRZY_ID })
        </td>
    </tr>
}

</table>
